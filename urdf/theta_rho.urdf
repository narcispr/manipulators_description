<?xml version="1.0"?>
<robot name="theta_rho">
  
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.1" radius="0.15"/>
      </geometry>
      <material name="grey"> 
        <color rgba="0.3 0.3 0.4 0.7"/>
      </material>
    </visual>
  </link>

  <link name="tmp11">
    <visual>
      <geometry>
        <cylinder length="0.0" radius="0.0"/>
      </geometry>
      <material name="dark_blue"> 
        <color rgba="0.0 0.0 0.5 0.5"/>
      </material>
    </visual>
  </link>

  <joint name="theta1" type="revolute">
    <parent link="base_link"/>
    <child link="tmp11"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="-3.1415" upper="3.1415" velocity="0.5"/>
    <origin rpy="0 0 1.57" xyz="0 0 0"/>
  </joint>

  <link name="tmp12">
    <visual>
      <geometry>
        <cylinder length="0.0" radius="0.0"/>
      </geometry>
      <material name="dark_blue"> 
        <color rgba="0.0 0.0 0.0 0.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.0" radius="0.0"/>
      </geometry>
    </collision>
  </link>

  <joint name="d1" type="fixed">
    <parent link="tmp11"/>
    <child link="tmp12"/>
    <origin rpy="0 0 0" xyz="0 0 0.5"/>
  </joint>

  <link name="tmp13">
    <visual>
      <geometry>
        <cylinder length="0.0" radius="0.0"/>
      </geometry>
      <material name="dark_blue"> 
        <color rgba="0.0 0.0 0.0 0.0"/>
      </material>
    </visual>
  </link>

  <joint name="a1" type="fixed">
    <parent link="tmp12"/>
    <child link="tmp13"/>
    <origin rpy="0 0 0" xyz="0.0 0 0"/>
  </joint>

 <link name="L1">
    <visual>
      <geometry>
        <cylinder length="0.1" radius="0.1"/>
      </geometry>
      <material name="dark_blue"> 
        <color rgba="0.0 0.0 0.0 0.0"/>
      </material>
    </visual>
  </link>

  <joint name="alpha1" type="fixed">
    <parent link="tmp13"/>
    <child link="L1"/>
    <origin rpy="1.57 0 0" xyz="0 0 0"/>
  </joint>



  <link name="tmp21">
    <visual>
      <geometry>
        <cylinder length="0.0" radius="0.0"/>
      </geometry>
      <material name="dark_blue"> 
        <color rgba="0.0 0.0 0.5 0.5"/>
      </material>
    </visual>
  </link>

  <joint name="theta2" type="fixed">
    <parent link="L1"/>
    <child link="tmp21"/>
    <origin rpy="0 0 1.57" xyz="0 0 0"/>
  </joint>

  <link name="tmp22">
    <visual>
      <geometry>
        <cylinder length="0.0" radius="0.0"/>
      </geometry>
      <material name="dark_blue"> 
        <color rgba="0.0 0.0 0.0 0.0"/>
      </material>
    </visual>
  </link>

  <joint name="d2" type="prismatic">
    <parent link="tmp21"/>
    <child link="tmp22"/>
    <limit effort="1000.0" lower="0.2" upper="1.0" velocity="0.5"/>
    <axis xyz="0 0 1"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>

  <link name="tmp23">
    <visual>
      <geometry>
        <cylinder length="0.0" radius="0.0"/>
      </geometry>
      <material name="dark_blue"> 
        <color rgba="0.0 0.0 0.0 0.0"/>
      </material>
    </visual>
  </link>

  <joint name="a2" type="fixed">
    <parent link="tmp22"/>
    <child link="tmp23"/>
    <origin rpy="0 0 0" xyz="0.0 0 0"/>
  </joint>

 <link name="L2">
    <visual>
      <geometry>
        <cylinder length="0.1" radius="0.1"/>
      </geometry>
      <material name="dark_blue"> 
        <color rgba="0.0 0.0 0.0 0.0"/>
      </material>
    </visual>
  </link>

  <joint name="alpha2" type="fixed">
    <parent link="tmp23"/>
    <child link="L2"/>
    <origin rpy="0.0 0 0" xyz="0 0 0"/>
  </joint>
</robot>
